{
  "name": "leazy",
  "version": "1.0.0",
  "description": "A simple web client for property and asset management",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build",
    "prestart": "run-s -s prebuild --build-commit=auto --deploy-env=localhost",
    "start": "webpack-dev-server --progress",
    "lint:js": "eslint --report-unused-disable-directives --ignore-path .gitignore .",
    "lint:md": "remark -i .gitignore --no-stdout --use remark-lint *.md",
    "jest": "jest --config ./jest.config.json --verbose --runInBand --no-cache ",
    "jest-dbg": "node --inspect-brk node_modules/.bin/jest --config ./jest.config.json --verbose --runInBand --no-cache",
    "lint": "run-s -s lint:*",
    "clean-build": "run-s clean build",
    "prebuild": "babel-node scripts/prebuild.js",
    "build": "webpack --display minimal --bail",
    "test": "run-s -s lint jest"
  },
  "repository": {
    "type": "git",
